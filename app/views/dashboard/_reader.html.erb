<div class="container-fluid">
  <div class="row">
    <main role="main" class="col-md-12 ml-sm-auto col-lg-12 px-md-4">
      <h2>Reader Notes</h2>
      <div class="table-responsive">
        <table class="table table-striped table-sm">
        <tr>
         <th>Title</th>
         <th>Body</th>
         <th>Created_by</th>
         <th>Action</th>  
      </tr>
      <% @allnotes_hash["Reader"].each do |role| %>
        <tr>
          <td><%= role.note.title %></td>
          <td><%= role.note.body %></td>
          <td><%= role.note.user.name%></td>
          <td><%= link_to 'Show', role.note %></td>
        </tr>
      <% end %>
        </table>
      </div>
    </main>
  </div>
</div>